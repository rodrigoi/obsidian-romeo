    "drizzle-orm": "^0.29.3",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "inngest": "^3.9.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^2.1.0",
    "typescript": "5.2.2",
    "zod": "^3.22.4"
  },
